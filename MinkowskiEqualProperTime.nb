 RowBox[{
  RowBox[{"(*", 
   RowBox[{"proper", "-", 
    RowBox[{"acceleration", " ", "profile"}]}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"a", "[", "tau_", "]"}], ":=", 
