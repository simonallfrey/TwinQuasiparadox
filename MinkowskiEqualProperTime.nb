           RowBox[{"{", 
            RowBox[{"0", ",", "0"}], "}"}], "]"}], ",", 
          RowBox[{"Directive", "[", 
           RowBox[{"Gray", ",", "Dashed"}], "]"}], ",", 
          RowBox[{"Line", "[", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{
               RowBox[{"-", "30"}], ",", 
               RowBox[{"-", "30"}]}], "}"}], ",", 
             RowBox[{"{", 
              RowBox[{"30", ",", "30"}], "}"}]}], "}"}], "]"}], ",", 
          RowBox[{"Line", "[", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{
               RowBox[{"-", "30"}], ",", "30"}], "}"}], ",", 
             RowBox[{"{", 
              RowBox[{"30", ",", 
               RowBox[{"-", "30"}]}], "}"}]}], "}"}], "]"}]}], "}"}], "]"}], 
       ",", "simultLines", ",", 
       RowBox[{"PlotRange", "->", 
        RowBox[{"{", 
         RowBox[{"xrange", ",", "trange"}], "}"}]}]}], "]"}]}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
