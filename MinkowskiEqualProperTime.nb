Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
    RowBox[{
    "Define", " ", "acceleration", " ", "and", " ", "solve", " ", "for", " ", 
     "x", 
     RowBox[{"(", "\[Tau]", ")"}]}], ",", 
    RowBox[{"t", 
     RowBox[{"(", "\[Tau]", ")"}], " ", "from", " ", "proper", " ", 
     "time"}]}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"Clear", "[", 
     RowBox[{"x", ",", "t", ",", "eta", ",", "a", ",", "tau"}], "]"}], ";"}], 
   "\n", 
   RowBox[{
    RowBox[{"g", "=", "9.80665"}], ";"}], "                ", 
   RowBox[{"(*", 
    RowBox[{"m", "/", 
     RowBox[{"s", "^", "2"}]}], "*)"}], "\n", 
   RowBox[{
    RowBox[{"c", "=", "299792458"}], ";"}], "              ", 
   RowBox[{"(*", 
    RowBox[{"m", "/", "s"}], "*)"}], "\n", 
   RowBox[{
    RowBox[{"yearSec", "=", 
     RowBox[{"365.25", "*", "24", "*", "3600"}]}], ";"}], "   ", 
   RowBox[{"(*", 
    RowBox[{"s", "/", "year"}], "*)"}], "\n", 
   RowBox[{
    RowBox[{"a0", "=", ".3"}], ";"}], "   ", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{
      RowBox[{"ly", "/", 
       RowBox[{"year", "^", "2"}]}], " ", "1", " ", 
      RowBox[{"ly", "/", "year"}]}], "==", "c"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{"ag", "=", 
    RowBox[{"a0", " ", "*", " ", "yearSec", " ", "*", " ", 
     RowBox[{
      RowBox[{"c", " ", "/", " ", 
       RowBox[{"yearSec", "^", "2"}]}], " ", "/", " ", "g", " ", 
      RowBox[{"(*", 
       RowBox[{"acceleration", " ", "in", " ", "g"}], 
       "*)"}]}]}]}]}]}]], "Input",
