,Cell["Second plot: instantaneous simultaneity lines through each marker.", "Text",
 CellChangeTimes->{{3.974398300000000*^9, 3.974398300000000*^9}},ExpressionUUID->"cbe3e042-690c-4bf0-b737-502a2dd0b027"],
Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"Simultaneity", " ", "lines", " ", "t", " ", "=", " ", "v", " ", 
     RowBox[{"x", " ", "+", " ", "b"}]}], ",", " ", 
    RowBox[{"v", " ", "=", " ", 
     RowBox[{"tanh", " ", "\[Eta]"}]}]}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "Npts", ",", "tauMax", ",", "tauMarks", ",", "pts", ",", "vAt", ",", 
      "bAt", ",", "xrange", ",", "trange", ",", "worldline", ",", 
      "simultLines"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Npts", "=", "10"}], ";", "\[IndentingNewLine]", 
     RowBox[{"tauMax", "=", "20"}], ";", "\[IndentingNewLine]", 
     RowBox[{"tauMarks", "=", 
      RowBox[{"Subdivide", "[", 
       RowBox[{"0", ",", "tauMax", ",", "Npts"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"pts", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"xCoord", "[", "#", "]"}], ",", 
          RowBox[{"tCoord", "[", "#", "]"}]}], "}"}], "&"}], "/@", 
       "tauMarks"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"vAt", "[", "tau_", "]"}], ":=", 
      RowBox[{"Tanh", "[", 
       RowBox[{"etaFun", "[", "tau", "]"}], "]"}]}], ";", 
     RowBox[{"(*", " ", 
      RowBox[{"instantaneous", " ", "velocity"}], " ", "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"bAt", "[", "tau_", "]"}], ":=", 
      RowBox[{
       RowBox[{"tCoord", "[", "tau", "]"}], "-", 
       RowBox[{
        RowBox[{"vAt", "[", "tau", "]"}], " ", 
        RowBox[{"xCoord", "[", "tau", "]"}]}]}]}], ";", 
     RowBox[{"(*", " ", 
      RowBox[{"intercept", " ", "for", " ", "t", "=", "v", " ", "x", "+", 
       RowBox[{"b", " ", "at", " ", "each", " ", "\[Tau]"}]}], " ", "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"xrange", "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"-", "5"}], ",", "15"}], "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"trange", "=", 
      RowBox[{"{", 
       RowBox[{"0", ",", "25"}], "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"worldline", "=", 
      RowBox[{"ParametricPlot", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"xCoord", "[", "tau", "]"}], ",", 
          RowBox[{"tCoord", "[", "tau", "]"}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"tau", ",", "0", ",", "tauMax"}], "}"}], ",", 
        RowBox[{"PlotStyle", "->", 
         RowBox[{"{", 
          RowBox[{"Thick", ",", "Blue"}], "}"}]}], ",", 
        RowBox[{"PlotRange", "->", 
         RowBox[{"{", 
          RowBox[{"xrange", ",", "trange"}], "}"}]}], ",", 
        RowBox[{"AxesLabel", "->", 
         RowBox[{"{", 
          RowBox[{"\"\<x (ly)\>\"", ",", "\"\<t (yr)\>\""}], "}"}]}], ",", 
        RowBox[{"AspectRatio", "->", "2"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"simultLines", "=", 
      RowBox[{"Graphics", "[", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"Module", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"v", ",", "b", ",", "x1", ",", "x2"}], "}"}], ",", 
           RowBox[{
            RowBox[{"v", "=", 
             RowBox[{"vAt", "[", "t", "]"}]}], ";", 
            RowBox[{"b", "=", 
             RowBox[{"bAt", "[", "t", "]"}]}], ";", "\[IndentingNewLine]", 
            RowBox[{"x1", "=", 
             RowBox[{"xrange", "[", 
              RowBox[{"[", "1", "]"}], "]"}]}], ";", 
            RowBox[{"x2", "=", 
             RowBox[{"xrange", "[", 
              RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"Directive", "[", 
               RowBox[{"Orange", ",", "Dashed", ",", "Thin"}], "]"}], ",", 
              RowBox[{"Line", "[", 
               RowBox[{"{", 
                RowBox[{
                 RowBox[{"{", 
                  RowBox[{"x1", ",", 
                   RowBox[{
                    RowBox[{"v", " ", "x1"}], "+", "b"}]}], "}"}], ",", 
                 RowBox[{"{", 
                  RowBox[{"x2", ",", 
                   RowBox[{
                    RowBox[{"v", " ", "x2"}], "+", "b"}]}], "}"}]}], "}"}], 
               "]"}], ",", "Black", ",", 
              RowBox[{"PointSize", "[", "0.016", "]"}], ",", 
              RowBox[{"Point", "[", 
               RowBox[{"{", 
                RowBox[{"0", ",", "b"}], "}"}], "]"}]}], "}"}]}]}], "]"}], 
         ",", 
         RowBox[{"{", 
          RowBox[{"t", ",", "tauMarks"}], "}"}]}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Show", "[", 
      RowBox[{
       "worldline", ",", 
       RowBox[{"Graphics", "[", 
        RowBox[{"{", 
